{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "axe/keyboard": "off",
    "no-protocol-relative-urls": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 118",
    "not firefox <= 118",
    "not ios_saf <= 17",
    "not safari <= 17"
  ]
}